  "license": "MIT",
  "private": false,
  "engines": ">=12",
  "scripts": {
    "test": "jest .",
    "lint": "tslint --project '.'",
    "prettify": "prettier --config ./.prettierrc.json --write './source/**/*.ts'",
    "sane": "yarn lint && yarn prettify && yarn test",
    "start": "ts-node source/main.ts | pino-pretty",
    "watch": "nodemon --watch 'source/**/*.ts' --ignore 'source/**/*.test.ts' --exec 'ts-node' source/main.ts dist/main.js",
    "build": "tsc"
  },
  "dependencies": {
    "apollo-server": "2.18.1",
    "apollo-server-express": "2.18.1",
    "body-parser": "1.19.0",
    "camelcase-keys": "^6.2.2",
    "dotenv": "8.2.0",
    "errorhandler": "1.5.1",
    "express-pino-logger": "5.0.0",
    "express-session": "1.17.1",
    "graphql": "14.5.0",
    "method-override": "3.0.0",
    "methods": "1.1.2",
    "mongodb": "3.6.2",
    "mongoose": "5.10.7",
    "node-fetch": "2.6.1",
    "pino": "6.6.1",
    "reflect-metadata": "0.1.13",
    "type-graphql": "0.17.3",
    "typegoose": "5.9.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/errorhandler": "^1.5.0",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-session": "^1.17.0",
    "@types/graphql": "14.5.0",
    "@types/jest": "26.0.14",
    "@types/methods": "^1.1.1",
    "@types/mongodb": "3.5.27",
    "@types/mongoose": "5.7.36",
    "@types/node": "14.11.2",
    "@types/supertest": "^2.0.10",
    "chai": "^4.2.0",
    "jest": "26.4.2",
    "nodemon": "1.19.0",
    "prettier": "1.17.0",
    "supertest": "^5.0.0",
    "ts-jest": "26.4.1",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
  }
}
